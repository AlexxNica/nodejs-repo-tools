{
  "name": "@google-cloud/nodejs-repo-tools",
  "version": "1.0.3",
  "description": "Tools used to maintain and test Node.js repositories in the GoogleCloudPlaftorm organization.",
  "repository": {
    "type": "git",
    "url": "https://github.com/GoogleCloudPlatform/nodejs-repo-tools.git"
  },
  "scripts": {
    "lint": "semistandard **/*.js",
    "check": "yarn check --strict-semver --integrity",
    "test": "yarn run lint && yarn run check"
  },
  "main": "src/index.js",
  "author": "Google Inc.",
  "license": "Apache-2.0",
  "contributors": [
    {
      "name": "Jason Dobry",
      "email": "jdobry@google.com"
    },
    {
      "name": "Ace Nassri",
      "email": "anassri@google.com"
    }
  ],
  "semistandard": {
    "globals": [
      "after",
      "afterEach",
      "before",
      "beforeEach",
      "describe",
      "it"
    ]
  },
  "engines": {
    "node": ">=4.3.2"
  },
  "dependencies": {
    "colors": "^1.1.2",
    "got": "^6.7.1",
    "proxyquire": "^1.7.11",
    "supertest": "^3.0.0"
  },
  "devDependencies": {
    "semistandard": "^10.0.0"
  }
}
